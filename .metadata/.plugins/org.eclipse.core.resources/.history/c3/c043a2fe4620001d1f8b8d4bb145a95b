package com.springcore.ci;

public class Person {
     private String name;
     private int id;
     private Certificate certi;
     
	public Person(String name, int id, Certificate certi) {
		super();
		this.name = name;
		this.id = id;
		this.certi=certi;
	}

	public Person() {
		super();
		// TODO Auto-generated constructor stub
	}

	@Override
	public String toString() {
		return "Person [name=" + name + ", id=" + id + ", certificate=" + certi.name + "]";
	}
     
	
     
}
   